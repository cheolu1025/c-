    {
    num++
    return num;
    }
    
    4.
    #include <iostream>
    int Multiplier(int num1);
    int Multiplier(int num1,int num2);
    int Multiplier(int num1,int num2 ,int num3);
    
    int main()
    {
      std::cout << Multiplier(1) << std::endl;
      std::cout << Multiplier(1,2) << std::endl;
      std::cout << Multiplier(1,2,3) << std::endl;
      return 0;
      
    }
    int Multiplier(int num1)
    {
      return num1;
    }
    int Multiplier(int num1,int num2)
    {
      return num1*num2;
    }
    int Multiplier(int num1,int num2 ,int num3);
    {
      return num1*num2*num3; 
    }
    
